while True:
     time.sleep(3) #Waits 5 minutes between requests
        newHtml = getLinks()
        linksList2 = extractLinks(newHtml)

     if linksList1 != linksList2:
         NewLinks = linksList2 - linksList1
         print("New links found BOI")
         targetItemsLink = findBestMatched(NewLinks)
         break
